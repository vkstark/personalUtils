# OpenAI Configuration
OPENAI_API_KEY=your-api-key-here

# Model Selection
# Options: gpt-4o, gpt-4o-mini, gpt-4.1, o3-mini, gpt-5
MODEL_NAME=gpt-4o

# Chat Settings
MAX_TOKENS=4096
TEMPERATURE=0.7
STREAM_RESPONSES=true

# Tool Settings
ENABLE_TOOLS=true
PARALLEL_TOOL_CALLS=false

# Agent Settings
MAX_AGENT_ITERATIONS=5
ENABLE_PLANNING=true

# Logging
LOG_LEVEL=INFO
LOG_FILE=chatsystem.log
